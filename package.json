{
  "name": "imovieproject",
  "version": "1.0.0",
  "description": "Online Movie Ticket Booking System",
  "scripts": {
    "dev": "concurrently \"npm run dev:frontend\" \"npm run dev:backend\"",
    "dev:frontend": "cd frntend && npm run dev",
    "dev:backend": "cd Backend && npm run dev",
    "install:all": "cd frntend && npm install && cd ../Backend && npm install",
    "build": "cd frntend && npm run build",
    "start": "cd Backend && npm start"
  },
  "keywords": [
    "movie",
    "booking",
    "fullstack"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "concurrently": "^8.2.2"
  }
}
